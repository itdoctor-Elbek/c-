#include <iostream>
#include <stack> 
#include <iterator>

using namespace std;

int main()
{
    srand((unsigned) time(NULL));

    stack<double> st;
    int size = 50;
    double sum = 0;
    for(int i = 0; i < size; i++){
        int random = rand() % 50;
        st.push(random);
        sum+=random;

    }
    st.pop();
    st.push(sum/size);
    while(!st.empty()) {
        cout << st.top()<<" ";
        st.pop();
    }

    return 0;
}
